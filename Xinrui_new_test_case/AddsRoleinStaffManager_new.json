{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://app.ontraport.com/#!/contact/listAll"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='chrome-header']/nav/ul/li[1]/span"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Account"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div/div[2]/div[2]/ul/li[2]/a/div[1]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ontraport_panel_action_new']/span[2]"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div[1]/div/div[2]/div/div/div[1]/div/div/input"
      },
      "text": "abc"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div[1]/div/div[2]/div/div/div[2]/div/div/button"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div[1]/div/div[2]/div/div/div[2]/div/div/*/*/*/*/*/*[.='Intern']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div[2]/div/div[2]/div/div/div/div/div[1]/div/div/a/span[1]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[3]/div[1]/div[4]/button[1]"
      }
    },
    {
      "type": "refresh"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[1]/div[5]/div/div/div/input"
      },
      "text": "abc\n"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div/table/thead/tr/th[1]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[2]/div/ul/li/a"
      }
    },
    {
      "type": "assertTextPresent",
      "text": "Confirm you would like to remove this role"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[2]/div/ul/div/input[1]"
      }
    }
